{
  "name": "ui-kit",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "description": "UI Primitives Template",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "main": "./dist/index.js",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
      ".": {
        "import": {
          "types": "./dist/index.d.ts",
          "default": "./dist/index.js"
        },
        "require": {
          "types": "./dist/index.d.ts",
          "default": "./dist/index.js"
        }
      }
    }
  },
  "sideEffects": false,
  "exports": {
    ".": "./src/index.ts"
  },
  "main": "src/index.ts",
  "module": "src/index.ts",
  "types": "src/index.ts",
  "files": ["dist"],
  "scripts": {
    "build": "tsdown"
  },
  "peerDependencies": {
    "@types/react": "*",
    "@types/react-dom": "*",
    "react": "^16.8 || ^17.0 || ^18.0 || ^19.0 || ^19.0.0-rc",
    "react-dom": "^16.8 || ^17.0 || ^18.0 || ^19.0 || ^19.0.0-rc"
  },
  "peerDependenciesMeta": {
    "@types/react": {
      "optional": true
    },
    "@types/react-dom": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/react": "19.1.0",
    "@types/react-dom": "19.1.1",
    "react": "19.1.0",
    "react-dom": "19.1.1",
    "tsdown": "0.12.9",
    "typescript": "5.8.2"
  }
}
