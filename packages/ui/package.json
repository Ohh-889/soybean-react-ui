{
  "name": "skyroc-ui",
  "type": "module",
  "version": "0.0.9",
  "private": false,
  "description": "A modern React UI component library built on Radix UI and Tailwind CSS",
  "author": {
    "name": "Ohh",
    "email": "1509326266@qq.com",
    "url": "https://github.com/Ohh-889"
  },
  "license": "MIT",
  "homepage": "https://github.com/Ohh-889/skyroc-ui/tree/master/packages/ui",
  "repository": {
    "url": "https://github.com/Ohh-889/skyroc-ui.git"
  },
  "bugs": {
    "url": "https://github.com/Ohh-889/skyroc-ui/issues"
  },
  "keywords": [
    "react",
    "ui",
    "components",
    "tailwind",
    "radix-ui",
    "skyroc",
    "design-system",
    "typescript",
    "react-components",
    "ui-library",
    "component-library"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "main": "./dist/index.js",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
      ".": {
        "import": {
          "types": "./dist/index.d.ts",
          "default": "./dist/index.js"
        }
      },
      "./*": {
        "types": "./dist/components/*/index.d.ts",
        "import": "./dist/components/*/index.js",
        "require": "./dist/components/*/index.js"
      },
      "./utils": {
        "types": "./dist/lib/index.d.ts",
        "import": "./dist/lib/index.js",
        "require": "./dist/lib/index.js"
      }
    }
  },
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./src/index.ts"
    }
  },
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "types": "./src/index.ts",
  "files": ["README.zh.md", "dist", "README.md"],
  "scripts": {
    "build": "pnpm run build:components && pnpm run registry",
    "build:components": "tsdown",
    "build:registry": "shadcn build -o ../../playground/public/r",
    "dev": "tsx script/fix-pkg.ts dev",
    "pgk:prod": "tsx script/fix-pkg.ts prod",
    "registry": "tsx script/registry.ts"
  },
  "dependencies": {
    "@iconify/react": "6.0.0",
    "@radix-ui/react-accordion": "1.2.11",
    "@radix-ui/react-alert-dialog": "1.1.14",
    "@radix-ui/react-aspect-ratio": "1.1.7",
    "@radix-ui/react-avatar": "1.1.10",
    "@radix-ui/react-checkbox": "1.3.2",
    "@radix-ui/react-collapsible": "1.1.11",
    "@radix-ui/react-compose-refs": "1.1.2",
    "@radix-ui/react-context-menu": "2.2.15",
    "@radix-ui/react-dialog": "1.1.14",
    "@radix-ui/react-dropdown-menu": "2.1.15",
    "@radix-ui/react-hover-card": "1.1.14",
    "@radix-ui/react-label": "2.1.7",
    "@radix-ui/react-menu": "2.1.15",
    "@radix-ui/react-menubar": "1.1.15",
    "@radix-ui/react-navigation-menu": "1.2.13",
    "@radix-ui/react-popover": "1.1.14",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-radio-group": "1.3.7",
    "@radix-ui/react-scroll-area": "1.2.9",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "1.1.7",
    "@radix-ui/react-slider": "1.3.5",
    "@radix-ui/react-slot": "1.2.3",
    "@radix-ui/react-switch": "1.2.5",
    "@radix-ui/react-tabs": "1.1.12",
    "@radix-ui/react-toggle": "1.1.9",
    "@radix-ui/react-toggle-group": "1.1.10",
    "@radix-ui/react-tooltip": "1.2.7",
    "@radix-ui/react-use-controllable-state": "1.2.2",
    "@skyroc/form": "workspace:*",
    "@skyroc/tailwind-plugin": "workspace:*",
    "@skyroc/utils": "workspace:*",
    "clsx": "2.1.1",
    "cmdk": "1.1.1",
    "embla-carousel-react": "8.6.0",
    "input-otp": "1.4.2",
    "lucide-react": "0.525.0",
    "next-themes": "0.4.6",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-resizable-panels": "^3.0.6",
    "sonner": "2.0.6",
    "tailwind-merge": "3.3.0",
    "tailwind-variants": "1.0.0",
    "vaul": "1.1.2"
  },
  "devDependencies": {
    "@turbo/gen": "2.5.0",
    "@types/node": "22.15.3",
    "@types/react": "19.1.0",
    "@types/react-dom": "19.1.1",
    "fast-glob": "3.3.3",
    "rimraf": "6.0.1",
    "shadcn": "3.5.0",
    "tsdown": "0.12.9",
    "tsx": "4.20.3",
    "typescript": "5.8.2"
  }
}
